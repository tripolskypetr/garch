{
  "name": "garch",
  "version": "1.2.0",
  "description": "GARCH and EGARCH volatility models for TypeScript",
  "type": "module",
  "main": "./build/index.cjs",
  "module": "./build/index.mjs",
  "types": "./types.d.ts",
  "exports": {
    ".": {
      "types": "./types.d.ts",
      "import": "./build/index.mjs",
      "require": "./build/index.cjs"
    }
  },
  "files": [
    "build",
    "types.d.ts",
    "README.md"
  ],
  "scripts": {
    "build": "rollup -c",
    "test": "vitest run",
    "test:watch": "vitest",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "garch",
    "egarch",
    "volatility",
    "finance",
    "econometrics",
    "time-series"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@rollup/plugin-typescript": "^12.3.0",
    "@types/node": "^20.10.0",
    "@vitest/coverage-v8": "^1.6.1",
    "rollup": "^4.57.1",
    "rollup-plugin-dts": "^6.3.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "tslib": "^2.8.1",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0"
  }
}
